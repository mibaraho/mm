.page.page-tiles
  .pageheader
    h2
      | {{page.title}} 
      span // {{page.subtitle}}
    .page-bar
      ul.page-breadcrumb
        li
          a(ui-sref='main.dashboard')
            i.fa.fa-home
            |  {{main.title}}
        li
          a(ui-sref='main.minutes.list') {{page.title}}
  // /page header
  // page content
  .pagecontent
    // row
    .row
      // col
      .col-md-12
        // tile
        section.tile.tile-simple.no-bg
          // tile body
          .tile-body.streamline.timeline.p-0
            ul
              li.heading
                a.btn.btn-greensea.b-0(href='javascript:;') February
                li.timeline-post
                  aside
                    .thumb.wh30.bg-lightred
                      i.fa.fa-map-marker
                  .post-container
                    .panel.panel-default.b-0
                      h3.custom-font.text-danger Meeting to deliver the final results
                      span.text-muted.time
                        i.fa.fa-clock-o
                        |  2015-03-11 11:45
                        i.fa.fa-map-marker
                        |  Client's office
                      p
                        | In this meeting were taken 3 agreements, 3 comments were made and...
                li.timeline-post
                  aside
                    .thumb.wh30.bg-blue
                      i.fa.fa-clock-o
                  .post-container
                    .panel.panel-default.b-0
                      h3.custom-font.text-blue Last meeting before delivering the results
                      span.text-muted.time
                        i.fa.fa-clock-o
                        |  2015-03-09 09:00
                      p
                        | We defined the last steps before...
                li.timeline-post
                  aside
                    .thumb.wh30.bg-greensea
                      i.fa.fa-rocket
                  .post-container
                    .panel.panel-default.b-0
                      h3.custom-font.text-greensea Neque porro quisquam est
                      span.text-muted.time
                        i.fa.fa-clock-o
                        |  12 hours ago
