                  label.form-control Client's office
              // /col
              // col
              .col-md-6
                p.input-group
                  input.form-control(type='text', datepicker-popup='dd-MM-yyyy', is-open='opened', show-button-bar="false", ng-model='meetingMinute.date', date-disabled='disabled(date, mode)', close-text='Close', disabled='disabled')
                  span.input-group-btn
                    button.btn.btn-default(type='button', ng-click='open($event)')
                      i.fa.fa-calendar
              // /col
            // /row
          // /tile widget
          // tile body
          .tile-body
            p This is tile body area.
