                button.btn.btn-rounded-20.btn-default.btn-sm(type='button', style='width:30px;')
                  i.fa.fa-eye(style='margin-left: -2px;')
              .media-body
                p.media-heading.mb-0.mt-10
                  | Deel 
                  strong McApple
                    small.text-lightred Print master
            hr
            .media
              .pull-left.thumb
                img.media-object.img-circle(src='assets/images/random-avatar8.jpg', alt='')
              .pull-right.mt-10
                button.btn.btn-rounded-20.btn-default.btn-sm.mr-5(type='button', style='width:30px;')
                  i.fa.fa-times
                button.btn.btn-rounded-20.btn-default.btn-sm.mr-5(type='button', style='width:30px;')
