        // /tile
        // tile
        section.tile.tile-simple
          // tile header
          .tile-header
            h1.custom-font
              strong My
              |  Projects
          // /tile header
          // tile body
          .tile-body
            ul.list-unstyled
              li
                .row
                  .col-md-3
                    | Minimal
                  .col-md-9
                    progressbar.progress-striped.not-rounded(value='56', type='warning') 56%
              li
                .row
                  .col-md-3
