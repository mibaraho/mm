              .pull-left.thumb
                img.media-object.img-circle(src='assets/images/random-avatar5.jpg', alt='')
              .pull-right.mt-10
                button.btn.btn-rounded-20.btn-default.btn-sm.mr-5(type='button', style='width:30px;')
