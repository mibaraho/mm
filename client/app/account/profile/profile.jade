.page.page-profile
  // page header
  .pageheader
    h2
      | {{page.title}} 
      span // {{page.subtitle}}
    .page-bar
      ul.page-breadcrumb
        li
          a(ui-sref='app.dashboard')
            i.fa.fa-home
            |  {{main.title}}
        li
          a(href='javascript:;') Profile
        li
          a(ui-sref='app.pages.profile') {{page.title}}
  // /page header
  // page content
  .pagecontent
    // row
    .row
      // col
      .col-md-4
        // tile
        section.tile.tile-simple
          // tile widget
          .tile-widget.p-30.text-center
            .thumb.thumb-xl
              img.img-circle(src='assets/images/arnold-avatar.jpg', alt='')
            h4.mb-0
              strong John
              |  Douey
            span.text-muted Project Manager
            .mt-10
              button.btn.btn-rounded-20.btn-sm.btn-greensea Follow
              button.btn.btn-rounded-20.btn-sm.btn-lightred Message
          // /tile widget
          // tile body
          .tile-body.text-center.bg-blue.p-0
            ul.list-inline.tbox.m-0
              li.tcol.p-10
                h2.m-0 695
                span.text-transparent-white Tweets
              li.tcol.bg-blue.dker.p-10
                h2.m-0 1 269
                span.text-transparent-white Followers
              li.tcol.p-10
                h2.m-0 369
                span.text-transparent-white Following
          // /tile body
        // /tile
        // tile
        section.tile.tile-simple
          // tile header
          .tile-header
            h1.custom-font
              strong Friend
              |  List
          // /tile header
          // tile body
          .tile-body
