                        input#zip.form-control(type='text', value='215 62')
                    .row
                      .form-group.col-sm-6
                        label(for='email') E-mail
                        input#email.form-control(type='email', value='johny.d@tattek.com', readonly='')
                      .form-group.col-sm-6
                        label(for='country') Country
                        select#country(ui-jq='chosen', ui-options="{width: '100%'}")
                          option Slovakia
                          option Czech Republic
                          option Poland
                          option Hungary
                          option Austria
                    .row
                      .form-group.col-sm-6
                        label(for='phone') Phone
                        input#phone.form-control(type='text', value='+(421) 903 669 478')
                        span.help-block +(421) 999 999 999
                      .form-group.col-sm-6
                        label(for='avatar') Avatar
                        input#avatar.filestyle(ui-jq='filestyle', type='file', ui-options="{\
                        buttonText: 'Find file',\
